    "nprogress": "^0.2.0",
    "qs": "^6.10.2",
    "vue": "^2.7.14",
    "vue-i18n": "^8.26.8",
    "vue-router": "^3.6.5",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.1",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-legacy": "^4.0.1",
